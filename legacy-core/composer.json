{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
           "type": "vcs",
            "url": "https://github.com/ControlpadLLC/cp-common.git"
         }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.3.*",
        "fideloper/proxy": "^3.2",
        "predis/predis": "~1.0",
        "laravelcollective/html": "^5.2",
        "camroncade/timezone": "0.1",
        "codeception/codeception": "2.2.1",
        "intervention/image": "2.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "picoprime/barcodegen": "@dev",
        "league/csv": "^8.1",
        "guzzlehttp/streams": "^3.0",
        "doctrine/dbal": "2.7.2",
        "lakshmajim/twilio": "^1.2",
        "torann/geoip": "^1.0",
        "rollbar/rollbar": "~1.1",
        "captbrogers/laravel-rollbar": "^2.0.0",
        "breeswish/php-marked": "^0.3.3",
        "laravel/socialite": "^2.0",
        "facebook/graph-sdk": "^5.4.4",
        "sofa/eloquence": "~5.3",
        "tymon/jwt-auth": "0.5.*",
        "toin0u/geocoder-laravel": "1.0.0-RC1@dev",
        "bmartus/laravel-shippo": "dev-master",
        "shippo/shippo-php": "1.3.0",
        "flow/jsonpath": "0.3.0@dev",
        "web/cp-common": "0.1.*"
    },
    "require-dev": {
        "codeception/codeception": "2.2.1",
        "phpunit/phpunit": "~5.0",
        "fzaninotto/faker": "~1.4",
        "guzzlehttp/guzzle": "~6.2",
        "mockery/mockery": "~0.9",
        "phpunit/php-code-coverage": "~4.0",
        "squizlabs/php_codesniffer": "~2.6.1",
        "codeception/c3": "~2.0",
        "symfony/css-selector": "~3.1",
        "symfony/dom-crawler": "~3.1",
        "symfony/psr-http-message-bridge": "0.2",
        "mpociot/laravel-apidoc-generator": "^1.7"
    },
    "autoload": {
        "classmap": [
            "database", "app/Http/Controllers"
        ],
        "files": [
            "app/Helpers/http_constants.php",
            "app/Helpers/globals.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
